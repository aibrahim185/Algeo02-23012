{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/next.config.ts",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/py/(.*)",
      "dest": "backend/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
